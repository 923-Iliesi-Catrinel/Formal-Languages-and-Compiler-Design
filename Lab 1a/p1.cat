fn main[]: void
{
    ~ Maximum of three integers
    @a: integer -> 10
    @b: integer -> 20
    @c: integer -> 40
    @max: integer -> @a

    if [@max < @b]
    {
        @max -> @b
    }
        
    if [@max < @c]
    {
        @max -> @c
    }

    write[@max, " is the maximum number out of the 3."]
}
